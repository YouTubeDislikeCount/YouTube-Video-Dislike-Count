{
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "YouTube Video Dislike Count",
    "description": "Brings back the dislike count for YouTube videos",
    "permissions": [
        "tabs",
        "<all_urls>",
        "storage"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}
